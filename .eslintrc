{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": ["error", {
            "objects": "never"
        }],
        "no-unused-vars": ["error"],
        "react/jsx-filename-extension": 0,
        "indent": ["error", 4],
        "no-undef": 0,
        "arrow-body-style": 0,
        "no-unused-expressions": 0
    }
}